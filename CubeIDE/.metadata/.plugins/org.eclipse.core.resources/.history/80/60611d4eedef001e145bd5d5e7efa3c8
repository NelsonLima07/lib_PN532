#include "usr/main_PN532.h"

#define TEMPO_LED_EXTERNO 1000 /* Tempo definido em milisegundos ms  */

int main_PN532 (void)
{
  uint32_t tmrLED = HAL_GetTick();

  while(1)
    {
      //tmrLED = HAL_GetTick();
      if (HAL_GetTick() - tmrLED >= TEMPO_LED_EXTERNO)
      {
	tmrLED = HAL_GetTick();
	HAL_GPIO_TogglePin(LED_EXTERNO_GPIO_Port, LED_EXTERNO_Pin);
	tmrLED = HAL_GetTick();
      }
    }

  //HAL_Delay(1000);

}
